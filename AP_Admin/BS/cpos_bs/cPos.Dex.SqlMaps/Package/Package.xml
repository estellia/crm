    <insert id="InsertPackage" parameterClass="PackageInfo">
      insert into t_dex_package
      (
      pkg_id
      ,pkg_type_id
      ,pkg_gen_type_id
      ,app_type_id
      ,unit_id
      ,customer_id
      ,user_id
      ,pkg_name
      ,pkg_desc
      ,pkg_seq
      ,pkg_status
      ,bat_id
      ,create_time
      ,create_user_id
      ,modify_time
      ,modify_user_id
      )
      values
      (
      #PkgId#
      ,#PkgTypeId#
      ,#PkgGenTypeId#
      ,#AppTypeId#
      ,#UnitId#
      ,#CustomerId#
      ,#UserId#
      ,#PkgName#
      ,#PkgDesc#
      ,#PkgSeq#
      ,#PkgStatus#
      ,#BatId#
      ,#CreateTime#
      ,#CreateUserId#
      ,#ModifyTime#
      ,#ModifyUserId#
      )
    </insert>

    <update id="UpdatePackage" parameterClass="PackageInfo">
      update t_dex_package
      set
      Pkg_Type_Id=#PkgTypeId#
      ,Pkg_Gen_Type_Id=#PkgGenTypeId#
      ,App_Type_Id=#AppTypeId#
      ,Unit_Id=#UnitId#
      ,Customer_Id=#CustomerId#
      ,User_Id=#UserId#
      ,Pkg_Name=#PkgName#
      ,Pkg_Desc=#PkgDesc#
      ,Pkg_Seq=#PkgSeq#
      ,Pkg_Status=#PkgStatus#
      ,Bat_Id=#BatId#
      ,Modify_Time=#ModifyTime#
      ,Modify_User_Id=#ModifyUserId#
      where pkg_id=#PkgId#
    </update>

    <update id="PublishPackage" parameterClass="PackageInfo">
      update t_dex_package
      set
      Pkg_Status='1'
      ,Modify_Time=#ModifyTime#
      ,Modify_User_Id=#ModifyUserId#
      where pkg_id=#PkgId# and Pkg_Status='0'
    </update>

    <delete id="DeletePackage" parameterClass="string">
      delete t_dex_package
      where pkg_id=#PkgId#
    </delete>

    
